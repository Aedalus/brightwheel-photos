{
  "name": "Brightwheel Image Grabber",
  "description": "Extract all images from current web page",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/hello_extensions.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["scripting", "activeTab"],
  "background": {}
}
